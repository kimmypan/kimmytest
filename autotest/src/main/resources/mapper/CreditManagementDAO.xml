<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.tdw.dao.CreditManagementDAO">
    <sql id="credit_message">
		identityState,confidence,verifyScore
	</sql>

    <select id="seleCreditManage"  parameterType="java.lang.String" resultType="cn.tdw.domain.CreditManagement">
        SELECT  <include refid="credit_message"/> FROM
        CreditManagement WHERE userId=#{userId}
    </select>



</mapper>