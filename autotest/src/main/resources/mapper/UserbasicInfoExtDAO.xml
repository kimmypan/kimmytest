<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.tdw.dao.UserBasicInfoExtDAO">


    <sql id="userBasicInfo_ext">
      frontCardBucket,frontCardObjectId,backCardBucket,backCardObjectId,livenessBucket,livenessObjectId
	</sql>

    <select id="SelecUserInfoExt" parameterType="java.lang.String" resultType="cn.tdw.domain.UserBasicInfoExt">
	      SELECT <include refid="userBasicInfo_ext"/> FROM UserBasicInfoExt WHERE userId=#{userId}
    </select>

</mapper>