<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.tdw.dao.ApiDataDAO">
    <resultMap id="apiDataMap" type="cn.tdw.domain.ApiData">
        <id column="dataId" property="dataId"/>
        <result column="functionCode"  property="functionCode"/>
        <result column="desc" property="desc"/>
        <result column="responseBusiData" property="responseBusiData"/>
        <result column="responseCode" property="responseCode"/>
        <result column="responseMsg" property="responseMsg"/>
        <result column="validTime" property="validTime"/>
        <result column="userId" property="userId"/>
        <result column="resultStatus" property="resultStatus"/>
        <result column="createDate" property="createDate"/>
        <result column="lastUpdated" property="lastUpdated"/>
        <result column="isDelete" property="isDelete"/>
        <result column="version" property="version"/>
    </resultMap>



    <sql id="apiData_zd">
    functionCode,responseBusiData,userId,createDate
	</sql>

    <select id="apiDataList" resultMap="apiDataMap"  parameterType="java.lang.String" >
        SELECT  <include refid="apiData_zd"/>
        FROM ApiData WHERE userId=#{userId} AND isDelete=0
    </select>




</mapper>